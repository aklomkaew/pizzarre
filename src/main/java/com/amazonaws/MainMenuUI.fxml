<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="720.0" minWidth="720.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.amazonaws.MainMenuUI">
   <children>
      <BorderPane prefHeight="720.0" prefWidth="720.0">
         <effect>
            <ColorAdjust />
         </effect>
         <bottom>
            <Button fx:id="logoutBtn" alignment="CENTER" mnemonicParsing="false" onAction="#logout" text="Logout" BorderPane.alignment="CENTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets bottom="25.0" />
               </BorderPane.margin>
               <effect>
                  <InnerShadow height="10.0" radius="4.5" width="10.0" />
               </effect>
            </Button>
         </bottom>
         <top>
            <TitledPane alignment="TOP_RIGHT" prefHeight="26.0" prefWidth="167.0" text="#UserName" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <VBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="newOrderBtn" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goToNewOrder" prefHeight="100.0" prefWidth="200.0" text="New Order" textAlignment="CENTER" wrapText="true">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                     <font>
                        <Font size="28.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
                  <Button fx:id="myOrdersBtn" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goToMyOrders" prefHeight="100.0" prefWidth="200.0" text="My Orders" textAlignment="CENTER" wrapText="true">
                     <VBox.margin>
                        <Insets bottom="50.0" top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font size="28.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
                  <Button fx:id="mUtilBtn" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goToManagerUtilities" prefHeight="100.0" prefWidth="200.0" text="Manager Utilities" textAlignment="CENTER" wrapText="true">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font size="28.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
               </BorderPane.margin>
            </VBox>
         </center></BorderPane>
   </children>
</AnchorPane>
